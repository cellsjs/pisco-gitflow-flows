{
  "name": "start",
  "description": "[Git flow]: Start new branch",
  "steps": {
    "start": {
      "implementation-check" : false
    },
    "setVersion": {
      "excludes" : ["develop", "master"],
      "params" : {
        "pretag" : "true"
      }
    },
    "push" : {
      "implementation-check" : false,
      "inputs" : {
        "branchName" : {"start": "branchName"}
      }
    }
  }
}
